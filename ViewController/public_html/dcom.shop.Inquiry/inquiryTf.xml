<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
    <task-flow-definition id="inquiryTf">
        <default-activity>inquiry</default-activity>
        <view id="inquiry">
            <page>/dcom.shop.Inquiry/page/inquiry.amx</page>
        </view>
        <view id="searchProduct">
            <page>/dcom.shop.Inquiry/page/searchProduct.amx</page>
        </view>
        <view id="ProductList">
            <page>/dcom.shop.Inquiry/page/ProductList.amx</page>
        </view>
        <view id="productDetails">
            <page>/dcom.shop.Inquiry/page/productDetails.amx</page>
        </view>
        <view id="lpnSearch">
            <page>/dcom.shop.Inquiry/page/lpnSearch.amx</page>
        </view>
        <view id="LpnList">
            <page>/dcom.shop.Inquiry/page/LpnList.amx</page>
        </view>
        <view id="LpnDetails">
            <page>/dcom.shop.Inquiry/page/LpnDetails.amx</page>
        </view>
        <control-flow-rule id="__1">
            <from-activity-id>inquiry</from-activity-id>
            <control-flow-case id="__2">
                <from-outcome>productSearch</from-outcome>
                <to-activity-id>searchProduct</to-activity-id>
            </control-flow-case>
            <control-flow-case id="__11">
                <from-outcome>lpnInquiry</from-outcome>
                <to-activity-id>lpnSearch</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__3">
            <from-activity-id>searchProduct</from-activity-id>
            <control-flow-case id="__4">
                <from-outcome>productlist</from-outcome>
                <to-activity-id>ProductList</to-activity-id>
            </control-flow-case>
            <control-flow-case id="__7">
                <from-outcome>searchBack</from-outcome>
                <to-activity-id>inquiry</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__5">
            <from-activity-id>ProductList</from-activity-id>
            <control-flow-case id="__6">
                <from-outcome>details</from-outcome>
                <to-activity-id>productDetails</to-activity-id>
            </control-flow-case>
            <control-flow-case id="__8">
                <from-outcome>productlistBack</from-outcome>
                <to-activity-id>searchProduct</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__12">
            <from-activity-id>lpnSearch</from-activity-id>
            <control-flow-case id="__13">
                <from-outcome>lpnInquiryBack</from-outcome>
                <to-activity-id>inquiry</to-activity-id>
            </control-flow-case>
            <control-flow-case id="__9">
                <from-outcome>lpnlist</from-outcome>
                <to-activity-id>LpnList</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__17">
            <from-activity-id>productDetails</from-activity-id>
            <control-flow-case id="__18">
                <from-outcome>detailsBack</from-outcome>
                <to-activity-id>searchProduct</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__10">
            <from-activity-id>LpnList</from-activity-id>
            <control-flow-case id="__14">
                <from-outcome>lpnlistBack</from-outcome>
                <to-activity-id>lpnSearch</to-activity-id>
            </control-flow-case>
            <control-flow-case id="__15">
                <from-outcome>lpnDetails</from-outcome>
                <to-activity-id>LpnDetails</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__16">
            <from-activity-id>LpnDetails</from-activity-id>
            <control-flow-case id="__19">
                <from-outcome>lpnDetailsBack</from-outcome>
                <to-activity-id>lpnSearch</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
    </task-flow-definition>
</adfc-mobile-config>
