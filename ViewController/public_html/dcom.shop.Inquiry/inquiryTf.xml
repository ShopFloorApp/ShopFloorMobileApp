<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
    <task-flow-definition id="inquiryTf">
        <default-activity>inquiry</default-activity>
        <view id="inquiry">
            <page>/dcom.shop.Inquiry/page/inquiry.amx</page>
        </view>
        <view id="searchProduct">
            <page>/dcom.shop.Inquiry/page/searchProduct.amx</page>
        </view>
        <view id="ProductList">
            <page>/dcom.shop.Inquiry/page/ProductList.amx</page>
        </view>
        <view id="productDetails">
            <page>/dcom.shop.Inquiry/page/productDetails.amx</page>
        </view>
        <control-flow-rule id="__1">
            <from-activity-id>inquiry</from-activity-id>
            <control-flow-case id="__2">
                <from-outcome>search</from-outcome>
                <to-activity-id>searchProduct</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__3">
            <from-activity-id>searchProduct</from-activity-id>
            <control-flow-case id="__4">
                <from-outcome>productlist</from-outcome>
                <to-activity-id>ProductList</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__5">
            <from-activity-id>ProductList</from-activity-id>
            <control-flow-case id="__6">
                <from-outcome>details</from-outcome>
                <to-activity-id>productDetails</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
    </task-flow-definition>
</adfc-mobile-config>
