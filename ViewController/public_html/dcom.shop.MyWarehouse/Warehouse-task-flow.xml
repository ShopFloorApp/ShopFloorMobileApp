<?xml version="1.0" encoding="UTF-8" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="Warehouse-task-flow">
    <default-activity>MyWarehouse</default-activity>
    <view id="MyWarehouse">
      <page>/dcom.shop.MyWarehouse/MyWarehouse.amx</page>
    </view>
    <task-flow-call id="dispatch">
      <task-flow-reference>
        <document>/dcom.shop.DispatchList/dispatch.xml</document>
        <id>dispatch</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>MyWarehouse</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>Dispatch</from-outcome>
        <to-activity-id>dispatch</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
