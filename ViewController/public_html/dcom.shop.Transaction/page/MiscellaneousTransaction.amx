                    <?audit suppress oracle.maf.warning-missing-label?>
                    <amx:selectItem label="Choose" value="0" id="si7"/>
                    <amx:selectItems value="#{bindings.subFromInv.items}" id="si8"/>
                </amx:selectOneChoice>
                <amx:selectOneChoice label="Locator" value="#{pageFlowScope.FromLocator}" id="soc5">
                    <?audit suppress oracle.maf.warning-missing-label?>
                    <amx:selectItem label="Choose" value="0" id="si6"/>
                    <amx:selectItems value="#{bindings.fromLoc.items}" id="si5"/>
                </amx:selectOneChoice>
                <amx:inputText label="Quantity" showRequired="true" inlineStyle="text-align:left;"
                               value="#{pageFlowScope.quantity}" styleClass="adfmf-listItem-captionText" id="it2">
                    <?audit suppress oracle.maf.warning-missing-hintText?>
                </amx:inputText>
            </amx:panelFormLayout>
            <amx:panelGroupLayout layout="horizontal" id="pgl1">
                <amx:spacer width="30" id="s7"/>
                <amx:commandLink id="cl4" rendered="#{!pageFlowScope.serialControl == 1}"
