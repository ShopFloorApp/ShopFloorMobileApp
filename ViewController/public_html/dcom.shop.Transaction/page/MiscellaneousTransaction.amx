                                <amx:outputText id="ot6" inlineStyle="font-weight:bold;color:#34AADC;"
                                                value="Lot Entry"/>
                            </amx:cellFormat>
                        </amx:rowLayout>
                    </amx:tableLayout>
                </amx:commandLink>
            </amx:panelGroupLayout>
        </amx:panelGroupLayout>
        <amx:facet name="footer">
            <amx:spacer id="s3" width="50"/>
            <amx:commandLink id="cl3" actionListener="#{StateListenerBean.ClearMiscTrxnPage}">
                <?audit suppress oracle.maf.warning-missing-shortDesc?>
                <amx:tableLayout id="tl1">
                    <amx:rowLayout id="rl1">
                        <amx:cellFormat id="cf1">
