                    </amx:rowLayout>
                </amx:tableLayout>
                <amx:setPropertyListener id="spl7" from="QUEUE" to="#{pageFlowScope.TrxnType}" type="action"></amx:setPropertyListener>
            </amx:commandLink>
        </amx:facet>
    </amx:panelPage>
</amx:view>
