                        <amx:outputText value="#{pageFlowScope.MiscTrxnSubinv}" styleClass="adfmf-listItem-captionText" id="it6"/>
                    </amx:panelLabelAndMessage>
                   <amx:setPropertyListener id="spl3" from="From" to="#{pageFlowScope.SubinventoryPage}"
                                             type="action"/>
                </amx:listItem>
                     <amx:listItem id="li7"
