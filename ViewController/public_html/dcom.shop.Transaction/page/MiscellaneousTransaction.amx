            <amx:spacer id="s4" width="20"/>
            <amx:commandLink id="cl6" action="bMM" actionListener="#{bindings.InsertMiscTransaction.execute}">
                <?audit suppress oracle.maf.warning-missing-shortDesc?>
                <amx:tableLayout id="tl4">
                    <amx:rowLayout id="rl4">
                        <amx:cellFormat id="cf6">
                            <?audit suppress oracle.maf.warning-missing-shortDesc?>
                            <amx:outputHtml id="oh4" styleClass="fa  fa-database fa-lg" inlineStyle="color:#34AADC;"/>
                            <?audit suppress oracle.ide.xml.validation-error?>
                            <amx:spacer id="s15" width="2"/>
                        </amx:cellFormat>
                        <amx:cellFormat id="cf4">
                            <amx:outputText id="ot7" value="Add to Queue"/>
