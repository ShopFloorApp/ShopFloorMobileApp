<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="ShippingTransactions">
    <default-activity>__ShippingTransactions2</default-activity>
    <managed-bean id="__7">
      <managed-bean-name>TransactionUtil</managed-bean-name>
      <managed-bean-class>dcom.shop.Transaction.bean.transactionUtil</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__24">
      <managed-bean-name>QuickShipUtilBean</managed-bean-name>
      <managed-bean-class>dcom.shop.Transaction.page.ShippingTxn.QuickShipUtilBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__28">
      <managed-bean-name>BarcodeBean</managed-bean-name>
      <managed-bean-class>dcom.shop.mobile.barcode.BarcodeBean</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__34">
      <managed-bean-name>UnloadTruckTxnBean</managed-bean-name>
      <managed-bean-class>dcom.shop.Transaction.page.ShippingTxn.UnloadTruckTxnBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__35">
      <managed-bean-name>StateListenerBean</managed-bean-name>
      <managed-bean-class>dcom.shop.application.mobile.StateListener</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <view id="PickRelease">
      <page>/dcom.shop.Transaction/page/ShippingTxn/PickRelease.amx</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <view id="__ShippingTransactions2">
      <page>/dcom.shop.Transaction/page/ShippingTxn/ShippingTransactions.amx</page>
    </view>
    <view id="LPNship">
      <page>/dcom.shop.Transaction/page/ShippingTxn/LPNship.amx</page>
    </view>
    <view id="DocumentNo">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/DocumentNo.amx</page>
    </view>
    <view id="LpnLOVPG">
      <page>/page/LovPage/LpnLOVPG.amx</page>
    </view>
    <view id="dockLOV">
      <page>/dcom.shop.Transaction/page/ShippingTxn/dockLOV.amx</page>
    </view>
    <view id="quickShip">
      <page>/dcom.shop.Transaction/page/ShippingTxn/quickShip.amx</page>
    </view>
    <view id="deliveryLOVPG">
      <page>/page/LovPage/deliveryLOVPG.amx</page>
    </view>
    <task-flow-call id="PackAndDirectShipTF">
      <task-flow-reference>
        <document>/dcom.shop.Transaction/page/PackAndDirectShipTF.xml</document>
        <id>PackAndDirectShipTF</id>
      </task-flow-reference>
    </task-flow-call>
    <view id="UnloadTruck">
      <page>/dcom.shop.Transaction/page/ShippingTxn/UnloadTruck.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>PickRelease</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>backToShipT</from-outcome>
        <to-activity-id>__ShippingTransactions2</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__2">
        <from-outcome>pickRLov</from-outcome>
        <to-activity-id>DocumentNo</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>backToTxn</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>__ShippingTransactions2</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>pickRel</from-outcome>
        <to-activity-id>PickRelease</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__16">
        <from-outcome>lpnShip</from-outcome>
        <to-activity-id>LPNship</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__18">
        <from-outcome>quickShip</from-outcome>
        <to-activity-id>quickShip</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__25">
        <from-outcome>PackAndDirectShip</from-outcome>
        <to-activity-id>PackAndDirectShipTF</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__29">
        <from-outcome>toUnloadTruck</from-outcome>
        <to-activity-id>UnloadTruck</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>LPNship</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>backToShipT</from-outcome>
        <to-activity-id>__ShippingTransactions2</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__11">
        <from-outcome>lpn</from-outcome>
        <to-activity-id>LpnLOVPG</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__17">
        <from-outcome>dock</from-outcome>
        <to-activity-id>dockLOV</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>DocumentNo</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>back</from-outcome>
        <to-activity-id>PickRelease</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__14">
      <from-activity-id>LpnLOVPG</from-activity-id>
      <control-flow-case id="__15">
        <from-outcome>Back</from-outcome>
        <to-activity-id>LPNship</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__19">
      <from-activity-id>quickShip</from-activity-id>
      <control-flow-case id="__20">
        <from-outcome>quickShipBack</from-outcome>
        <to-activity-id>__ShippingTransactions2</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__21">
        <from-outcome>delvLov</from-outcome>
        <to-activity-id>deliveryLOVPG</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__22">
      <from-activity-id>deliveryLOVPG</from-activity-id>
      <control-flow-case id="__23">
        <from-outcome>backToPage</from-outcome>
        <to-activity-id>quickShip</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__26">
      <from-activity-id>PackAndDirectShipTF</from-activity-id>
      <control-flow-case id="__27">
        <from-outcome>backToShipT</from-outcome>
        <to-activity-id>__ShippingTransactions2</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__30">
      <from-activity-id>UnloadTruck</from-activity-id>
      <control-flow-case id="__31">
        <from-outcome>unloadDock</from-outcome>
        <to-activity-id>dockLOV</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__32">
        <from-outcome>unloadLPN</from-outcome>
        <to-activity-id>LpnLOVPG</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__33">
        <from-outcome>backToShipTxn</from-outcome>
        <to-activity-id>__ShippingTransactions2</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
