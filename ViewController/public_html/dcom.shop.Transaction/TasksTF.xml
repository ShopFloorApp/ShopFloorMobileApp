<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="TasksTF">
    <default-activity>TaskListPG</default-activity>
    <managed-bean id="__3">
      <managed-bean-name>TasksBean</managed-bean-name>
      <managed-bean-class>dcom.shop.Transaction.bean.tasksBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="TaskListPG">
      <page>/dcom.shop.Transaction/page/TaskListPG.amx</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>Back</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
