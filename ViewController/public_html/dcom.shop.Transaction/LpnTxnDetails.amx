        <amx:facet name="footer">
         <amx:spacer id="s1" width="50"/>
           
        <amx:commandLink id="cl2" rendered="#{pageFlowScope.LpnPage == 'PACK_FROM'}" actionListener="#{bindings.InsertTransaction.execute}">
