<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="ReceivingTransaction">
    <default-activity>ReceivingType</default-activity>
    <input-parameter-definition id="__8">
      <name>receivingType</name>
      <value>#{pageFlowScope.receivingType}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <managed-bean id="__5">
      <managed-bean-name>ReceivingTxnUtilBean</managed-bean-name>
      <managed-bean-class>dcom.shop.Transaction.page.bean.ReceivingTxnUtilBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <task-flow-return id="backToMain">
      <outcome>
        <name>backToMain</name>
      </outcome>
    </task-flow-return>
    <view id="ReceivingType">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/ReceivingType.amx</page>
    </view>
    <view id="DocumentNo">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/DocumentNo.amx</page>
    </view>
    <view id="lineDetails">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/lineDetails.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>backToMain</from-outcome>
        <to-activity-id>backToMain</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>ReceivingType</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>getValue</from-outcome>
        <to-activity-id>DocumentNo</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>addLine</from-outcome>
        <to-activity-id>lineDetails</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
