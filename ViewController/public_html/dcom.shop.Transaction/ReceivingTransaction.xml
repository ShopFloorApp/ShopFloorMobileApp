<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="ReceivingTransaction">
    <default-activity>Receiving</default-activity>
    <managed-bean id="__5">
      <managed-bean-name>ReceivingTxnUtilBean</managed-bean-name>
      <managed-bean-class>dcom.shop.Transaction.page.bean.ReceivingTxnUtilBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__30">
      <managed-bean-name>StateListenerBean</managed-bean-name>
      <managed-bean-class>dcom.shop.application.mobile.StateListener</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <task-flow-return id="backToMain">
      <outcome>
        <name>backToMain</name>
      </outcome>
    </task-flow-return>
    <view id="ReceivingType">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/ReceivingType.amx</page>
    </view>
    <view id="DocumentNo">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/DocumentNo.amx</page>
    </view>
    <view id="lineDetails">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/lineDetails.amx</page>
    </view>
    <method-call id="executeShipmentLines">
      <method>#{bindings.executeShipmentLines.execute}</method>
      <outcome>
        <fixed-outcome>back</fixed-outcome>
      </outcome>
    </method-call>
    <view id="LotEntry">
      <page>/page/LotEntry.amx</page>
    </view>
    <view id="SerialEntry">
      <page>/page/SerialEntry.amx</page>
    </view>
    <method-call id="getPendingReceiveTxn">
      <method>#{bindings.getPendingReceiveTxn.execute}</method>
      <outcome>
        <fixed-outcome>getPendingReceiveTxn</fixed-outcome>
      </outcome>
    </method-call>
    <view id="PendingReceiveTxn">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/PendingReceiveTxn.amx</page>
    </view>
    <router id="router1">
      <case id="__23">
        <expression>#{pageFlowScope.ReceiveTxnId==0}</expression>
        <outcome>createTxn</outcome>
      </case>
      <case id="__27">
        <expression>#{pageFlowScope.ReceiveTxnId!=0}</expression>
        <outcome>pendingList</outcome>
      </case>
      <default-outcome>createTxn</default-outcome>
    </router>
    <view id="successPage">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/successPage.amx</page>
    </view>
    <method-call id="getPendingReceiveTxn1">
      <method>#{bindings.getPendingReceiveTxn.execute}</method>
      <outcome>
        <fixed-outcome>getPendingReceiveTxn</fixed-outcome>
      </outcome>
    </method-call>
    <view id="LinesListPG">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/LinesListPG.amx</page>
    </view>
    <view id="Receiving">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/Receiving.amx</page>
    </view>
    <view id="ReceiveLPN">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/ReceiveLPN.amx</page>
    </view>
    <view id="LPNList">
      <page>/dcom.shop.Transaction/page/ReceivingTxn/LPNList.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>backToMain</from-outcome>
        <to-activity-id>backToMain</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__44">
        <from-outcome>ReceiveHome</from-outcome>
        <to-activity-id>Receiving</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>ReceivingType</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>getValue</from-outcome>
        <to-activity-id>DocumentNo</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>addLine</from-outcome>
        <to-activity-id>lineDetails</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__28">
        <from-outcome>pendingTxn</from-outcome>
        <to-activity-id>getPendingReceiveTxn</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__31">
        <from-outcome>success</from-outcome>
        <to-activity-id>successPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>DocumentNo</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>back</from-outcome>
        <to-activity-id>executeShipmentLines</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>executeShipmentLines</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>back</from-outcome>
        <to-activity-id>ReceivingType</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__14">
      <from-activity-id>lineDetails</from-activity-id>
      <control-flow-case id="__15">
        <from-outcome>lot</from-outcome>
        <to-activity-id>LotEntry</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__16">
        <from-outcome>serial</from-outcome>
        <to-activity-id>SerialEntry</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__29">
        <from-outcome>back</from-outcome>
        <to-activity-id>ReceivingType</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__17">
      <from-activity-id>SerialEntry</from-activity-id>
      <control-flow-case id="__18">
        <from-outcome>Back</from-outcome>
        <to-activity-id>lineDetails</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__19">
      <from-activity-id>LotEntry</from-activity-id>
      <control-flow-case id="__20">
        <from-outcome>Back</from-outcome>
        <to-activity-id>lineDetails</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>PendingReceiveTxn</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>new</from-outcome>
        <to-activity-id>ReceivingType</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__32">
        <from-outcome>success</from-outcome>
        <to-activity-id>successPage</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__40">
        <from-outcome>self</from-outcome>
        <to-activity-id>PendingReceiveTxn</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__21">
      <from-activity-id>getPendingReceiveTxn</from-activity-id>
      <control-flow-case id="__22">
        <from-outcome>getPendingReceiveTxn</from-outcome>
        <to-activity-id>router1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__24">
      <from-activity-id>router1</from-activity-id>
      <control-flow-case id="__25">
        <from-outcome>createTxn</from-outcome>
        <to-activity-id>ReceivingType</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__26">
        <from-outcome>pendingList</from-outcome>
        <to-activity-id>PendingReceiveTxn</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__33">
      <from-activity-id>successPage</from-activity-id>
      <control-flow-case id="__34">
        <from-outcome>que</from-outcome>
        <to-activity-id>getPendingReceiveTxn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__35">
      <from-activity-id>getPendingReceiveTxn1</from-activity-id>
      <control-flow-case id="__36">
        <from-outcome>getPendingReceiveTxn</from-outcome>
        <to-activity-id>PendingReceiveTxn</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__37">
      <from-activity-id>LinesListPG</from-activity-id>
      <control-flow-case id="__38">
        <from-outcome>back</from-outcome>
        <to-activity-id>ReceivingType</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__39">
        <from-outcome>createLine</from-outcome>
        <to-activity-id>lineDetails</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__41">
      <from-activity-id>Receiving</from-activity-id>
      <control-flow-case id="__42">
        <from-outcome>receiveMaterial</from-outcome>
        <to-activity-id>getPendingReceiveTxn</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__43">
        <from-outcome>lpn</from-outcome>
        <to-activity-id>ReceiveLPN</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__45">
      <from-activity-id>ReceiveLPN</from-activity-id>
      <control-flow-case id="__46">
        <from-outcome>getLPN</from-outcome>
        <to-activity-id>LPNList</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__47">
      <from-activity-id>LPNList</from-activity-id>
      <control-flow-case id="__48">
        <from-outcome>back</from-outcome>
        <to-activity-id>ReceiveLPN</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
