<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.13.52" id="instructionsPageDef"
                Package="dcom.shop.DispatchList.page">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="root" RangeSize="25" DataControl="InstructionsDC" id="InstructionsDCIterator"/>
    <accessorIterator MasterBinding="InstructionsDCIterator" Binds="instructionsBO" RangeSize="25"
                      DataControl="InstructionsDC" BeanClass="dcom.shop.application.mobile.dispatch.InstructionsBO"
                      id="instructionsBOIterator"/>
  </executables>
  <bindings>
    <tree IterBinding="instructionsBOIterator" id="instructionsBO">
      <nodeDefinition DefName="dcom.shop.application.mobile.dispatch.InstructionsBO" Name="instructionsBO0">
        <AttrNames>
          <Item Value="FILENAME"/>
          <Item Value="URL"/>
          <Item Value="CATEGDESC"/>
          <Item Value="TITLE"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <methodAction id="displayFile" RequiresUpdateModel="true" Action="invokeMethod" MethodName="displayFile"
                  IsViewObjectMethod="false" DataControl="DeviceFeatures"
                  InstanceName="data.DeviceFeatures.dataProvider">
      <NamedData NDName="fileURL" NDValue="#{pageFlowScope.filePath}" NDType="java.lang.String"/>
      <NamedData NDName="headerText" NDValue="Attachment File" NDType="java.lang.String"/>
    </methodAction>
    <methodAction id="getBlobData" RequiresUpdateModel="true" Action="invokeMethod" MethodName="getBlobData"
                  IsViewObjectMethod="false" DataControl="AttachmentDC" InstanceName="data.AttachmentDC.dataProvider"/>
  </bindings>
</pageDefinition>
