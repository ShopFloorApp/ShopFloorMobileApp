<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.13.52" id="transactComponentPageDef"
                Package="dcom.shop.DispatchList.page">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="root" RangeSize="25" DataControl="TransactDC" id="TransactDCIterator"/>
    <accessorIterator MasterBinding="TransactDCIterator" Binds="transact" RangeSize="25" DataControl="TransactDC"
                      BeanClass="dcom.shop.application.mobile.dispatch.TransactBO" id="transactIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="OpSeqDC" id="OpSeqDCIterator"/>
    <accessorIterator MasterBinding="OpSeqDCIterator" Binds="opSeq" RangeSize="-1" DataControl="OpSeqDC"
                      BeanClass="dcom.shop.application.mobile.dispatch.OpSeqBO" id="opSeqIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="ItemDC" id="ItemDCIterator"/>
    <accessorIterator MasterBinding="ItemDCIterator" Binds="allItems" RangeSize="-1" DataControl="ItemDC"
                      BeanClass="dcom.shop.application.mobile.ItemBO" id="allItemsIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="SubinventoryDC" id="SubinventoryDCIterator"/>
    <accessorIterator MasterBinding="SubinventoryDCIterator" Binds="subinventories" RangeSize="-1"
                      DataControl="SubinventoryDC" BeanClass="dcom.shop.application.mobile.SubinventoryBO"
                      id="subinventoriesIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="LocatorDC" id="LocatorDCIterator"/>
    <accessorIterator MasterBinding="LocatorDCIterator" Binds="locators" RangeSize="-1" DataControl="LocatorDC"
                      BeanClass="dcom.shop.application.mobile.LocatorBO" id="locatorsIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="UOMDC" id="UOMDCIterator"/>
    <accessorIterator MasterBinding="UOMDCIterator" Binds="UOM" RangeSize="-1" DataControl="UOMDC"
                      BeanClass="dcom.shop.application.mobile.UOMBO" id="UOMIterator"/>
  </executables>
  <bindings>
    <attributeValues IterBinding="transactIterator" id="trxQty">
      <AttrNames>
        <Item Value="trxQty"/>
      </AttrNames>
    </attributeValues>
    <methodAction id="saveTransaction" RequiresUpdateModel="true" Action="invokeMethod" MethodName="saveTransaction"
                  IsViewObjectMethod="false" DataControl="TransactDC" InstanceName="data.TransactDC.dataProvider"
                  ReturnName="data.TransactDC.methodResults.saveTransaction_TransactDC_dataProvider_saveTransaction_result">
      <NamedData NDName="transactBo" NDValue="#{bindings.transactIterator.currentRow.dataProvider}"
                 NDType="dcom.shop.application.mobile.dispatch.TransactBO"/>
    </methodAction>
    <list IterBinding="transactIterator" StaticList="true" id="trxType">
      <AttrNames>
        <Item Value="trxType"/>
      </AttrNames>
      <ValueList>
        <Item Value="WIP Issue"/>
        <Item Value="WIP Return"/>
      </ValueList>
    </list>
    <list StaticList="false" IterBinding="transactIterator" ListIter="opSeqIterator" id="fromOpSeq"
          NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
          NullValueFlag="start">
      <AttrNames>
        <Item Value="fromOpSeq"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="opSeq"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="opSeq"/>
      </ListDisplayAttrNames>
    </list>
    <list StaticList="false" IterBinding="transactIterator" ListIter="allItemsIterator" id="item">
      <AttrNames>
        <Item Value="item"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="inventoryItemId"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="itemNum"/>
      </ListDisplayAttrNames>
    </list>
    <list StaticList="false" IterBinding="transactIterator" ListIter="subinventoriesIterator" id="subinv">
      <AttrNames>
        <Item Value="subinv"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="subinv"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="subinv"/>
      </ListDisplayAttrNames>
    </list>
    <list StaticList="false" IterBinding="transactIterator" ListIter="locatorsIterator" id="locator">
      <AttrNames>
        <Item Value="locator"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="locator"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="locator"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="transactIterator" id="UOM" DTSupportsMRU="true" SelectItemValueMode="ListObject" StaticList="false"
          ListIter="UOMIterator">
      <AttrNames>
        <Item Value="txnUom"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="UOMCode"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="UOM"/>
      </ListDisplayAttrNames>
    </list>
    <methodAction id="refresh" RequiresUpdateModel="true" Action="invokeMethod" MethodName="refresh"
                  IsViewObjectMethod="false" DataControl="LocatorDC" InstanceName="data.LocatorDC.dataProvider"/>
    <methodAction id="rollback" RequiresUpdateModel="true" Action="invokeMethod" MethodName="rollback"
                  IsViewObjectMethod="false" DataControl="TransactDC" InstanceName="data.TransactDC.dataProvider"/>
  </bindings>
</pageDefinition>
