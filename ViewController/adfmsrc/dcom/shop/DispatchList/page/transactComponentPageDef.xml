<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.13.52" id="transactComponentPageDef"
                Package="dcom.shop.DispatchList.page">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="root" RangeSize="25" DataControl="TransactDC" id="TransactDCIterator"/>
    <accessorIterator MasterBinding="TransactDCIterator" Binds="transact" RangeSize="25" DataControl="TransactDC"
                      BeanClass="dcom.shop.application.mobile.dispatch.TransactBO" id="transactIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="OpSeqDC" id="OpSeqDCIterator"/>
    <accessorIterator MasterBinding="OpSeqDCIterator" Binds="opSeq" RangeSize="-1" DataControl="OpSeqDC"
                      BeanClass="dcom.shop.application.mobile.dispatch.OpSeqBO" id="opSeqIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="ItemDC" id="ItemDCIterator"/>
    <accessorIterator MasterBinding="ItemDCIterator" Binds="allItems" RangeSize="-1" DataControl="ItemDC"
                      BeanClass="dcom.shop.application.mobile.ItemBO" id="allItemsIterator"/>
  </executables>
  <bindings>
    <attributeValues IterBinding="transactIterator" id="subinv">
      <AttrNames>
        <Item Value="subinv"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="transactIterator" id="locator">
      <AttrNames>
        <Item Value="locator"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="transactIterator" id="trxQty">
      <AttrNames>
        <Item Value="trxQty"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="transactIterator" id="txnUom">
      <AttrNames>
        <Item Value="txnUom"/>
      </AttrNames>
    </attributeValues>
    <methodAction id="saveTransaction" RequiresUpdateModel="true" Action="invokeMethod" MethodName="saveTransaction"
                  IsViewObjectMethod="false" DataControl="TransactDC" InstanceName="data.TransactDC.dataProvider"
                  ReturnName="data.TransactDC.methodResults.saveTransaction_TransactDC_dataProvider_saveTransaction_result">
      <NamedData NDName="transactBo" NDValue="#{bindings.transactIterator.currentRow.dataProvider}"
                 NDType="dcom.shop.application.mobile.dispatch.TransactBO"/>
    </methodAction>
    <list IterBinding="transactIterator" StaticList="true" id="trxType">
      <AttrNames>
        <Item Value="trxType"/>
      </AttrNames>
      <ValueList>
        <Item Value="WIP Issue"/>
        <Item Value="WIP Return"/>
      </ValueList>
    </list>
    <list StaticList="false" IterBinding="transactIterator" ListIter="opSeqIterator" id="fromOpSeq">
      <AttrNames>
        <Item Value="fromOpSeq"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="opSeq"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="opSeq"/>
      </ListDisplayAttrNames>
    </list>
    <list StaticList="false" IterBinding="transactIterator" ListIter="allItemsIterator" id="item">
      <AttrNames>
        <Item Value="item"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="inventoryItemId"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="itemNum"/>
      </ListDisplayAttrNames>
    </list>
  </bindings>
</pageDefinition>
