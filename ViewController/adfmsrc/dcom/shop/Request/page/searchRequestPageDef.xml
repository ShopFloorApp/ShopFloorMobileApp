<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.2.36" id="searchRequestPageDef"
                Package="dcom.shop.Request.page">
    <parameters/>
    <executables>
        <variableIterator id="variables">
            <variable Name="pragramName" Type="java.lang.String"/>
        </variableIterator>
        <iterator Binds="root" RangeSize="25" DataControl="ConcurrentProgramDC" id="ConcurrentProgramDCIterator"/>
        <accessorIterator MasterBinding="ConcurrentProgramDCIterator" Binds="concurrentPrograms" RangeSize="-1"
                          DataControl="ConcurrentProgramDC"
                          BeanClass="dcom.shop.application.mobile.txn.ConcurrentProgramBO"
                          id="concurrentProgramsIterator"/>
        <accessorIterator id="filteredConcurrentProgramsIterator" MasterBinding="ConcurrentProgramDCIterator"
                          Binds="filteredConcurrentPrograms" RangeSize="-1" DataControl="ConcurrentProgramDC"
                          BeanClass="dcom.shop.application.mobile.txn.ConcurrentProgramBO"/>
    </executables>
    <bindings>
        <methodAction id="gotoSpringboard" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoSpringboard"
                      IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                      InstanceName="data.ApplicationFeatures.dataProvider"/>
        <list IterBinding="variables" id="name" DTSupportsMRU="true" SelectItemValueMode="ListObject"
              StaticList="false" ListIter="filteredConcurrentProgramsIterator">
            <AttrNames>
                <Item Value="pragramName"/>
            </AttrNames>
            <ListAttrNames>
                <Item Value="name"/>
            </ListAttrNames>
            <ListDisplayAttrNames>
                <Item Value="name"/>
            </ListDisplayAttrNames>
        </list>
        <methodAction id="refreshFilteredConcurrentPrograms" RequiresUpdateModel="true" Action="invokeMethod"
                      MethodName="refreshFilteredConcurrentPrograms" IsViewObjectMethod="false"
                      DataControl="ConcurrentProgramDC" InstanceName="data.ConcurrentProgramDC.dataProvider"/>
        <methodAction id="searchRequests" RequiresUpdateModel="true" Action="invokeMethod" MethodName="searchRequests"
                      IsViewObjectMethod="false" DataControl="ConcurrentProgramDC"
                      InstanceName="data.ConcurrentProgramDC.dataProvider"/>
    </bindings>
</pageDefinition>
