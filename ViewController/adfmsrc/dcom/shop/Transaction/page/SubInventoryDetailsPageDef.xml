<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.13.52" id="SubInventoryDetailsPageDef"
                Package="dcom.shop.Transaction.page">
  <parameters/>
  <executables>
    <variableIterator id="variables" Refresh="always">
      <variable Name="FromSubinv" Type="java.lang.String"/>
      <variable Name="FromLocator" Type="java.lang.String"/>
      <variable Name="ToSubinv" Type="java.lang.String"/>
      <variable Name="ToLocator" Type="java.lang.String"/>
    </variableIterator>
    <iterator Binds="root" RangeSize="25" DataControl="SubinventoryDC" id="SubinventoryDCIterator"/>
    <accessorIterator MasterBinding="SubinventoryDCIterator" Binds="subinventories" RangeSize="-1"
                      DataControl="SubinventoryDC" BeanClass="dcom.shop.application.mobile.SubinventoryBO"
                      id="subinventoriesIterator" RefreshCondition="#{pageFlowScope.refreshFromSubinventory ne 'N'}"/>
    <iterator Binds="root" RangeSize="25" DataControl="LocatorDC" id="LocatorDCIterator"/>
    <accessorIterator MasterBinding="LocatorDCIterator" Binds="locators" RangeSize="-1" DataControl="LocatorDC"
                      BeanClass="dcom.shop.application.mobile.LocatorBO" id="locatorsIterator" Refresh="always"/>
    <accessorIterator MasterBinding="LocatorDCIterator" Binds="toLocators" RangeSize="-1" DataControl="LocatorDC"
                      BeanClass="dcom.shop.application.mobile.LocatorBO" id="toLocatorsIterator"/>
    <accessorIterator MasterBinding="SubinventoryDCIterator" Binds="toSubinventories" RangeSize="-1"
                      DataControl="SubinventoryDC" BeanClass="dcom.shop.application.mobile.SubinventoryBO"
                      id="toSubinventoriesIterator" RefreshCondition="#{pageFlowScope.refreshToSubinventory ne 'N'}"/>
  </executables>
  <bindings>
    <methodAction id="gotoSpringboard" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoSpringboard"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider"/>
    <methodAction id="invokeInvTrnDC" RequiresUpdateModel="true" Action="invokeMethod" MethodName="ProcessWS"
                  IsViewObjectMethod="false" DataControl="InvTrnDC" InstanceName="data.InvTrnDC.dataProvider"/>
    <methodAction id="refresh" RequiresUpdateModel="true" Action="invokeMethod" MethodName="refresh"
                  IsViewObjectMethod="false" DataControl="LocatorDC" InstanceName="data.LocatorDC.dataProvider"/>
    <methodAction id="refreshToLocators" RequiresUpdateModel="true" Action="invokeMethod" MethodName="refreshToLocators"
                  IsViewObjectMethod="false" DataControl="LocatorDC" InstanceName="data.LocatorDC.dataProvider"/>
    <list IterBinding="SubinventoryDCIterator" id="subFromInv" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="subinventoriesIterator">
      <AttrNames>
        <Item Value="subInv"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="subinv"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="subinv"/>
        <Item Value="description"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="SubinventoryDCIterator" id="subToInv" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="toSubinventoriesIterator">
      <AttrNames>
        <Item Value="subToInv"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="subinv"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="subinv"/>
        <Item Value="description"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="LocatorDCIterator" id="fromLoc" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="locatorsIterator">
      <AttrNames>
        <Item Value="fromLoc"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="locator"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="locator"/>
        <Item Value="description"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="LocatorDCIterator" id="toLoc" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="toLocatorsIterator">
      <AttrNames>
        <Item Value="toLoc"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="locator"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="locator"/>
        <Item Value="description"/>
      </ListDisplayAttrNames>
    </list>
    <methodAction id="InsertTransaction" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="InsertTransaction" IsViewObjectMethod="false" DataControl="InvTrnDC"
                  InstanceName="data.InvTrnDC.dataProvider"
                  ReturnName="data.InvTrnDC.methodResults.InsertTransaction_InvTrnDC_dataProvider_InsertTransaction_result">
      <NamedData NDName="status" NDValue="#{pageFlowScope.TrxnType}" NDType="java.lang.String"/>
    </methodAction>
  </bindings>
</pageDefinition>
