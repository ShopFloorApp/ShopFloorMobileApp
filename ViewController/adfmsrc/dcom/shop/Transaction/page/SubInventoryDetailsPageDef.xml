<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.13.52" id="SubInventoryDetailsPageDef"
                Package="dcom.shop.Transaction.page">
  <parameters/>
  <executables>
    <variableIterator id="variables">
      <variable Name="FromSubinv" Type="java.lang.String"/>
      <variable Name="FromLocator" Type="java.lang.String"/>
      <variable Name="ToSubinv" Type="java.lang.String"/>
      <variable Name="ToLocator" Type="java.lang.String"/>
    </variableIterator>
    <iterator Binds="root" RangeSize="25" DataControl="SubinventoryDC" id="SubinventoryDCIterator"/>
    <accessorIterator MasterBinding="SubinventoryDCIterator" Binds="subinventories" RangeSize="-1"
                      DataControl="SubinventoryDC" BeanClass="dcom.shop.application.mobile.SubinventoryBO"
                      id="subinventoriesIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="LocatorDC" id="LocatorDCIterator"/>
    <accessorIterator MasterBinding="LocatorDCIterator" Binds="locators" RangeSize="-1" DataControl="LocatorDC"
                      BeanClass="dcom.shop.application.mobile.LocatorBO" id="locatorsIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="ItemDC" id="ItemDCIterator"/>
  </executables>
  <bindings>
    <list IterBinding="variables" id="subinv" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="subinventoriesIterator">
      <AttrNames>
        <Item Value="FromSubinv"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="subinv"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="subinv"/>
        <Item Value="desc"/>
      </ListDisplayAttrNames>
    </list>
    <methodAction id="gotoSpringboard" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoSpringboard"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider"/>
    <list IterBinding="variables" id="locator" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="locatorsIterator">
      <AttrNames>
        <Item Value="FromLocator"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="locator"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="locator"/>
        <Item Value="desc"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="variables" id="locator1" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="locatorsIterator">
      <AttrNames>
        <Item Value="ToLocator"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="locator"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="locator"/>
        <Item Value="desc"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="variables" id="subinv1" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="subinventoriesIterator">
      <AttrNames>
        <Item Value="ToSubinv"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="subinv"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="subinv"/>
        <Item Value="desc"/>
      </ListDisplayAttrNames>
    </list>
    <attributeValues IterBinding="ItemDCIterator" id="itemNum">
      <AttrNames>
        <Item Value="itemNum"/>
      </AttrNames>
    </attributeValues>
  </bindings>
</pageDefinition>
