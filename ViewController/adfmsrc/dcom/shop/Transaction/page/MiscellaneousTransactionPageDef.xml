<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.13.52"
                id="MiscellaneousTransactionPageDef" Package="dcom.shop.Transaction.page">
  <parameters/>
  <executables>
    <variableIterator id="variables">
      <variable Name="p_AccountAlias" Type="java.lang.String"/>
      <variable Name="p_TrxType" Type="java.lang.String"/>
    </variableIterator>
    <iterator Binds="root" RangeSize="25" DataControl="AccountAliasDC" id="AccountAliasDCIterator"/>
    <accessorIterator MasterBinding="AccountAliasDCIterator" Binds="accountAlias" RangeSize="-1"
                      DataControl="AccountAliasDC" BeanClass="dcom.shop.application.mobile.AccountAliasBO"
                      id="accountAliasIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="LocatorDC" id="LocatorDCIterator"/>
    <accessorIterator MasterBinding="LocatorDCIterator" Binds="locators" RangeSize="25" DataControl="LocatorDC"
                      BeanClass="dcom.shop.application.mobile.LocatorBO" id="locatorsIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="SubinventoryDC" id="SubinventoryDCIterator"/>
    <accessorIterator MasterBinding="SubinventoryDCIterator" Binds="subinventories" RangeSize="25"
                      DataControl="SubinventoryDC" BeanClass="dcom.shop.application.mobile.SubinventoryBO"
                      id="subinventoriesIterator"/>
  </executables>
  <bindings>
    <list IterBinding="variables" id="accountAlias" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="accountAliasIterator">
      <AttrNames>
        <Item Value="p_AccountAlias"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="accountAlias"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="accountAlias"/>
        <Item Value="accountSegment"/>
      </ListDisplayAttrNames>
    </list>
    <methodAction id="invokeGetSubInvByOrgIdDC" RequiresUpdateModel="true" Action="invokeMethod" MethodName="ProcessWS"
                  IsViewObjectMethod="false" DataControl="SubinventoryDC"
                  InstanceName="data.SubinventoryDC.dataProvider"/>
    <methodAction id="invokeLocatorDC" RequiresUpdateModel="true" Action="invokeMethod" MethodName="ProcessWS"
                  IsViewObjectMethod="false" DataControl="LocatorDC" InstanceName="data.LocatorDC.dataProvider">
      <NamedData NDName="subInv" NDValue="#{pageFlowScope.MiscTrxnSubinv}" NDType="java.lang.String"/>
    </methodAction>
    <list IterBinding="locatorsIterator" ListOperMode="navigation" ListIter="locatorsIterator" id="locators"
          DTSupportsMRU="true" SelectItemValueMode="ListObject">
      <AttrNames>
        <Item Value="locator"/>
        <Item Value="desc"/>
      </AttrNames>
    </list>
    <list IterBinding="subinventoriesIterator" ListOperMode="navigation" ListIter="subinventoriesIterator"
          id="subinventories" DTSupportsMRU="true" SelectItemValueMode="ListObject">
      <AttrNames>
        <Item Value="subinv"/>
        <Item Value="desc"/>
      </AttrNames>
    </list>
    <list IterBinding="variables" StaticList="true" id="TrxTypeList">
      <AttrNames>
        <Item Value="p_TrxType"/>
      </AttrNames>
      <ValueList>
        <Item Value="Miscellaneous Receipts"/>
        <Item Value="Miscellaneous Issues"/>
      </ValueList>
    </list>
    <methodAction id="invokeInvTrnDC" RequiresUpdateModel="true" Action="invokeMethod" MethodName="ProcessMiscTrxnWS"
                  IsViewObjectMethod="false" DataControl="InvTrnDC" InstanceName="data.InvTrnDC.dataProvider">
      <NamedData NDName="trxType" NDValue="#{pageFlowScope.TrxType}" NDType="java.lang.String"/>
    </methodAction>
    <attributeValues IterBinding="subinventoriesIterator" id="FromSubInv">
      <AttrNames>
        <Item Value="subinv"/>
      </AttrNames>
    </attributeValues>
  </bindings>
</pageDefinition>
