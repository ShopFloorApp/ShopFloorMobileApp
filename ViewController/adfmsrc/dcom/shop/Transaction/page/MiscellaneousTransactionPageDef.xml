<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.13.52"
                id="MiscellaneousTransactionPageDef" Package="dcom.shop.Transaction.page">
  <parameters/>
  <executables>
    <variableIterator id="variables">
      <variable Name="p_AccountAlias" Type="java.lang.String"/>
      <variable Name="p_TrxType" Type="java.lang.String"/>
    </variableIterator>
    <iterator Binds="root" RangeSize="25" DataControl="AccountAliasDC" id="AccountAliasDCIterator"/>
    <accessorIterator MasterBinding="AccountAliasDCIterator" Binds="accountAlias" RangeSize="-1"
                      DataControl="AccountAliasDC" BeanClass="dcom.shop.application.mobile.AccountAliasBO"
                      id="accountAliasIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="LocatorDC" id="LocatorDCIterator"/>
    <accessorIterator MasterBinding="LocatorDCIterator" Binds="locators" RangeSize="-1" DataControl="LocatorDC"
                      BeanClass="dcom.shop.application.mobile.LocatorBO" id="locatorsIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="SubinventoryDC" id="SubinventoryDCIterator"/>
    <accessorIterator MasterBinding="SubinventoryDCIterator" Binds="subinventories" RangeSize="-1"
                      DataControl="SubinventoryDC" BeanClass="dcom.shop.application.mobile.SubinventoryBO"
                      id="subinventoriesIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="UOMDC" id="UOMDCIterator"/>
    <accessorIterator MasterBinding="UOMDCIterator" Binds="UOM" RangeSize="-1" DataControl="UOMDC"
                      BeanClass="dcom.shop.application.mobile.UOMBO" id="UOMIterator"/>
  </executables>
  <bindings>
    <list IterBinding="AccountAliasDCIterator" id="accountAlias" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="accountAliasIterator">
      <AttrNames>
        <Item Value="accountAlias"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="accountAlias"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="description"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="variables" StaticList="true" id="TrxTypeList">
      <AttrNames>
        <Item Value="p_TrxType"/>
      </AttrNames>
      <ValueList>
        <Item Value="Account Alias Receipt"/>
        <Item Value="Account Alias Issue"/>
      </ValueList>
    </list>
    <list IterBinding="SubinventoryDCIterator" id="subFromInv" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="subinventoriesIterator">
      <AttrNames>
        <Item Value="subInv"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="subinv"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="subinv"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="LocatorDCIterator" id="fromLoc" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="locatorsIterator">
      <AttrNames>
        <Item Value="fromLoc"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="locator"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="locator"/>
      </ListDisplayAttrNames>
    </list>
    <methodAction id="gotoSpringboard" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoSpringboard"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider"/>
    <methodAction id="refresh" RequiresUpdateModel="true" Action="invokeMethod" MethodName="refresh"
                  IsViewObjectMethod="false" DataControl="LocatorDC" InstanceName="data.LocatorDC.dataProvider"/>
    <methodAction id="InsertMiscTransaction" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="InsertMiscTransaction" IsViewObjectMethod="false" DataControl="InvTrnDC"
                  InstanceName="data.InvTrnDC.dataProvider"
                  ReturnName="data.InvTrnDC.methodResults.InsertMiscTransaction_InvTrnDC_dataProvider_InsertMiscTransaction_result">
      <NamedData NDName="trxType" NDValue="#{pageFlowScope.TrxnType}" NDType="java.lang.String"/>
    </methodAction>
    <list IterBinding="UOMDCIterator" id="UOMCode" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="UOMIterator">
      <AttrNames>
        <Item Value="UOM"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="UOMCode"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="UOM"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="AccountAliasDCIterator" id="accountSegment" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="accountAliasIterator">
      <AttrNames>
        <Item Value="accountAlias"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="accountSegment"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="description"/>
      </ListDisplayAttrNames>
    </list>
    <methodAction id="callOnHandService" RequiresUpdateModel="true" Action="invokeMethod" MethodName="callOnHandService"
                  IsViewObjectMethod="false" DataControl="OnHandSearchDC"
                  InstanceName="data.OnHandSearchDC.dataProvider"/>
  </bindings>
</pageDefinition>
