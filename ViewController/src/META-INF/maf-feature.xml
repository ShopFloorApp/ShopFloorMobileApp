<?xml version="1.0" encoding="UTF-8" ?>
<adfmf:features xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:adfmf="http://xmlns.oracle.com/adf/mf">
    <adfmf:feature id="dcom.shop.syncPref" image="images/Update Filled-64.png" name="Sync" securityEnabled="true">
        <adfmf:content id="dcom.shop.syncPref.1">
            <adfmf:constraints/>
            <adfmf:amx file="dcom.shop.syncPref/syncPrefTF.xml#syncPrefTF">
                <adfmf:includes>
                    <adfmf:include type="JavaScript" file="resources/js/customSearch.js" id="i1"/>
                    <adfmf:include type="JavaScript" file="resources/js/Navigation.js" id="i2"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.LoadLov" name="LoadLov" securityEnabled="false">
        <adfmf:content id="dcom.shop.LoadLov.1">
            <adfmf:amx file="page/LovFragmentTest.amx">
                <adfmf:includes>
                    <adfmf:include type="JavaScript" file="resources/js/customSearch.js" id="i3"/>
                    <adfmf:include type="JavaScript" file="resources/js/Navigation.js" id="i4"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.subInventryTrsf" name="subInventryTrsf" securityEnabled="true">
        <adfmf:content id="dcom.shop.subInventryTrsf.1">
            <adfmf:amx file="page/SubInventoryTrsf.amx"/>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.DispatchList" image="images/In Transit-64.png" name="Dispatch List"
                   securityEnabled="true">
        <adfmf:content id="dcom.shop.DispatchList.1">
            <adfmf:amx file="dcom.shop.DispatchList/dispatch.xml#dispatch">
                <adfmf:includes>
                    <adfmf:include type="JavaScript" file="dcom.shop.DispatchList/js/openJob.js" id="i5"/>
                    <adfmf:include type="StyleSheet" file="dcom.shop.DispatchList/css/openJob.css" id="i6"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.Transaction" image="images/POS Terminal-64.png" name="Transaction"
                   securityEnabled="true">
        <adfmf:content id="dcom.shop.Transaction.1">
            <adfmf:constraints>
                <adfmf:constraint property="device.os" operator="equal" value="Android" id="c2"/>
            </adfmf:constraints>
            <adfmf:amx file="dcom.shop.Transaction/transactionTf.xml#transactionTf">
                <adfmf:includes>
                    <adfmf:include type="JavaScript" file="resources/js/expandCollapse.js" id="i7"/>
                    <adfmf:include type="JavaScript" file="resources/js/customSearch.js" id="i8"/>
                    <adfmf:include type="JavaScript" file="plugins/BarcodeScanner/Android/js/barcodescanner.js"
                                   id="i14"/>
                    <adfmf:include type="StyleSheet" file="resources/css/WorkBetter.css" id="i9"/>
                    <adfmf:include type="JavaScript" file="plugins/BarcodeScanner/Android/js/barcodescanner.js"
                                   id="i10"/>
                    <adfmf:include type="JavaScript" file="resources/js/Alert.js" id="i17"/>
                    <adfmf:include type="JavaScript" file="resources/js/customEvent.js" id="i20"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
        <adfmf:content id="dcom.shop.Transaction.2">
            <adfmf:constraints>
                <adfmf:constraint property="device.os" operator="equal" value="iOS" id="c1"/>
            </adfmf:constraints>
            <adfmf:amx file="dcom.shop.Transaction/transactionTf.xml#transactionTf">
                <adfmf:includes>
                    <adfmf:include type="StyleSheet" file="resources/css/WorkBetter.css" id="i15"/>
                    <adfmf:include type="JavaScript" file="resources/js/customSearch.js" id="i11"/>
                    <adfmf:include type="JavaScript" file="plugins/BarcodeScanner/iOS/js/barcodescanner.js" id="i12"/>
                    <adfmf:include type="JavaScript" file="resources/js/expandCollapse.js" id="i13"/>
                      <adfmf:include type="JavaScript" file="resources/js/Alert.js" id="i18"/>
              </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.Inquiry" image="images/Survey-64.png" name="Inquiry" securityEnabled="true">
        <adfmf:content id="dcom.shop.Inquiry.1">
            <adfmf:amx file="dcom.shop.Inquiry/inquiryTf.xml#inquiryTf">
                <adfmf:includes>
                    <adfmf:include type="JavaScript" file="resources/js/Alert.js" id="i19"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.Settings" image="images/Settings-64.png" name="Settings" securityEnabled="true">
        <adfmf:content id="dcom.shop.Settings.1">
            <adfmf:amx file="dcom.shop.Settings/settingsTf.xml#settingsTf"/>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.Preferences" name="Preferences" securityEnabled="true">
        <adfmf:content id="dcom.shop.Preferences.1">
            <adfmf:amx file="dcom.shop.Preferences/preferencesTf.xml#preferencesTf"/>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.Springboard" name="Springboard" securityEnabled="true">
        <adfmf:content id="dcom.shop.Springboard.1">
            <adfmf:amx file="dcom.shop.Springboard/springboard.amx"/>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.Request" image="images/Print-64.png" name="Request" securityEnabled="true">
        <adfmf:content id="dcom.shop.Request.1">
            <adfmf:amx file="dcom.shop.Request/requestTf.xml#requestTf">
                <adfmf:includes>
                    <adfmf:include type="JavaScript" file="resources/js/customEvent.js" id="i16"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.SecuredFeature" image="images/Exit-64.png" name="Sign Out" securityEnabled="true">
        <adfmf:content id="dcom.shop.SecuredFeature.1">
            <adfmf:amx file="dcom.shop.SecuredFeature/SecuredFeature-task-flow.xml#SecuredFeature-task-flow"/>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.MyWarehouse" name="Warehouse" securityEnabled="true" image="images/Warehouse-64.png">
        <adfmf:content id="dcom.shop.MyWarehouse.1">
            <adfmf:amx file="dcom.shop.MyWarehouse/Warehouse-task-flow.xml#Warehouse-task-flow"/>
        </adfmf:content>
        <adfmf:preferences>
            <adfmf:preferenceGroup id="OrgCodePG" label="Org Code PG">
                <adfmf:preferenceText id="OrgCode" label="Org Code"/>
            </adfmf:preferenceGroup>
        </adfmf:preferences>
    </adfmf:feature>
    <adfmf:feature id="dcom.shop.DefaultTaskFlow" name="DefaultTaskFlow" securityEnabled="true">
        <adfmf:content id="dcom.shop.DefaultTaskFlow.1">
            <adfmf:amx file="dcom.shop.DefaultTaskFlow/Default-task-flow.xml#Default-task-flow"/>
        </adfmf:content>
    </adfmf:feature>
</adfmf:features>